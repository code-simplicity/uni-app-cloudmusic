	// 自动引入comments文件夹下的组件，根目录
	"easycom": {
		"autoscan": true,
		"custom": {
			"^sq-(.*)": "@/components/sq/sq-$1.vue", // 匹配components目录内的vue文件
			"^q-(.*)": "@/components/q/$1/q-$1.vue", // 匹配components目录内的vue文件
			"^my-(.*)": "@/components/home/$1/my-$1.vue", // 匹配components/home/$的目录内的vue文件
			"^recommend-(.*)": "@/components/home/recommend-$1/recommend-$1.vue", // 匹配components/home/$的目录内的vue文件
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue", //匹配uview组件
			"^music-(.*)": "@/components/music/music-$1/music-$1.vue", // 匹配components目录内的vue文件
			"^scroll-(.*)": "@/components/scroll/scroll.vue", // 匹配components目录内的vue文件
			"^board-(.*)": "@/components/home/board-$1/board-$1.vue", // 匹配components目录内的vue文件
			"^user-(.*)": "@/components/user/user-$1/user-$1.vue", // 匹配components目录内的vue文件
			"^play-(.*)": "@/components/play-list/play-$1/play-$1.vue", // 匹配components目录内的vue文件
			"^search-(.*)": "@/components/search/search-$1/search-$1.vue", // 匹配components目录内的vue文件
			"^comment-(.*)": "@/components/comment/comment-$1/comment-$1.vue", // 匹配components目录内的vue文件
			"^focus-(.*)": "@/components/focus/focus-$1/focus-$1.vue", // 匹配components目录内的vue文件
			"^radio-station-(.*)": "@/components/radio-station/radio-station-$1/radio-station-$1.vue", // 匹配components目录内的vue文件
			"^video-(.*)": "@/components/video/video-$1/video-$1.vue", // 匹配components目录内的vue文件
			"^lyric-(.*)": "@/components/lyric/lyric-$1.vue", // 匹配components目录内的vue文件
			"^leader-(.*)": "@/components/leader-board/leader-$1/leader-$1.vue" // 匹配components目录内的vue文件
		}

	}
